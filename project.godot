; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GameOfLife"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.3")

[autoload]

Llama="*res://source/llama/llama.gd"
Quiz="*res://games/quiz/nodes/quiz.gd"
Variables="*res://source/autoload/variables.gd"
Todos="*res://todos.gd"
Logger="*res://source/autoload/logger.gd"
Constants="*res://source/autoload/constants.gd"
Templates="*res://source/autoload/templates.gd"
Queue="*res://source/autoload/queue.gd"
SceneSettings="*res://source/autoload/scene_settings.gd"

[display]

window/size/viewport_height=1024
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://games/quiz/": "orange",
"res://games/reference/": "yellow",
"res://games/standup/": "green",
"res://source/": "red"
}
